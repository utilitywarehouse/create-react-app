{
  "name": "william-example-app",
  "version": "0.0.0",
  "description": "React APP for William",
  "author": "web-systems@utilitywarehouse.co.uk",
  "main": "src/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start:server": "node server",
    "eject": "react-scripts eject",
    "test": "jest"
  },
  "moduleRoots": [
    "src/web/",
    "src/modules/",
    "src/screens/",
    "src/components/"
  ],
  "jest": {
    "modulePaths": [
      "src/web/",
      "src/modules/",
      "src/screens/",
      "src/components/"
    ],
    "transform": {
      "^.+\\.js": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/node_modules/react-native-web"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTestsFramework.js"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "^17.0.2",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-root-import": "^4.1.3",
    "chai": "^3.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-scripts": "^1.0.11",
    "jest": "^22.1.2",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "@utilitywarehouse/william-redux": "^0.1.14",
    "@utilitywarehouse/william-sdk": "^0.1.28",
    "@utilitywarehouse/william-ui": "^0.1.28",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native-web": "^0.1.7",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.6.0"
  }
}
